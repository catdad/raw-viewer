{
  "name": "raw-viewer",
  "productName": "Raw Viewer",
  "version": "1.0.0-beta0",
  "description": "raw-viewer",
  "main": "main.js",
  "scripts": {
    "postinstall": "electron-rebuild && node scripts/exiftool-install.js",
    "lint": "eslint main.js lib public renderer scripts test",
    "pretest": "node test/lib/downloader.js",
    "test": "mocha --timeout 10000 --slow 0 test/**.test.js",
    "citest": "xvfb-maybe npm test && npm run -s lint",
    "start": "electron .",
    "dev": "electronmon .",
    "exif-kill": "taskkill /IM exiftool.exe /F",
    "exif-find": "tasklist | findstr exiftool",
    "package": "node scripts/build-package.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/catdad/raw-viewer.git"
  },
  "author": "Kiril Vatev <vatev.1@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/catdad/raw-viewer/issues"
  },
  "homepage": "https://github.com/catdad/raw-viewer#readme",
  "dependencies": {
    "dcraw": "^1.0.3",
    "fast-deep-equal": "^2.0.1",
    "fs-extra": "^7.0.1",
    "lodash": "^4.17.11",
    "node-exiftool": "^2.3.0",
    "p-throttle": "^2.1.0",
    "pretty-bytes": "^5.1.0",
    "sharp": "^0.21.3",
    "trash": "^4.3.0"
  },
  "devDependencies": {
    "archiver": "^3.0.0",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "del": "^4.1.0",
    "devtron": "^1.4.0",
    "electron": "^3.1.3",
    "electron-debug": "^2.1.0",
    "electron-installer-zip": "^0.1.2",
    "electron-packager": "^13.1.1",
    "electron-rebuild": "^1.8.4",
    "electronmon": "^0.2.0",
    "eslint": "^4.19.1",
    "form-data": "^2.3.3",
    "mocha": "^6.0.2",
    "node-fetch": "^2.3.0",
    "rootrequire": "^1.0.0",
    "spectron": "^5.0.0",
    "tar": "^4.4.8",
    "tempy": "^0.2.1",
    "unzip": "^0.1.11",
    "xvfb-maybe": "^0.2.1",
    "yargs-parser": "^13.0.0"
  }
}
