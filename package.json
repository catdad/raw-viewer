{
  "name": "raw-viewer",
  "productName": "Raw Viewer",
  "version": "1.0.0-beta6",
  "description": "raw-viewer",
  "main": "main.js",
  "scripts": {
    "postinstall": "electron-rebuild && node scripts/exiftool-install.js && node scripts/fonts-install.js",
    "lint": "eslint main.js lib public renderer scripts test",
    "pretest": "node test/lib/downloader.js",
    "test": "mocha --timeout 30000 --slow 0 test/**.test.js",
    "citest": "xvfb-maybe npm test && npm run -s lint",
    "start": "electron .",
    "dev": "electronmon .",
    "exif-kill": "taskkill /IM exiftool.exe /F",
    "exif-find": "tasklist | findstr exiftool",
    "package": "node scripts/build-package.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/catdad/raw-viewer.git"
  },
  "author": "Kiril Vatev <vatev.1@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/catdad/raw-viewer/issues"
  },
  "homepage": "https://github.com/catdad/raw-viewer#readme",
  "dependencies": {
    "ag-psd": "^6.2.0",
    "dcraw": "^1.0.3",
    "dcraw-vendored-darwin": "9.28.1",
    "dcraw-vendored-linux": "9.28.1",
    "dcraw-vendored-win32": "9.28.1",
    "fast-deep-equal": "^2.0.1",
    "fs-extra": "^8.0.0",
    "fs-watch-file": "^1.0.0",
    "lodash": "^4.17.11",
    "node-fetch": "^2.6.0",
    "p-throttle": "^3.1.0",
    "pretty-bytes": "^5.2.0",
    "semver": "^6.1.0",
    "sharp": "^0.22.1",
    "trash": "^5.2.0"
  },
  "devDependencies": {
    "archiver": "^3.0.0",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "del": "^4.1.1",
    "devtron": "^1.4.0",
    "electron": "^5.0.6",
    "electron-debug": "^2.2.0",
    "electron-packager": "^13.1.1",
    "electron-rebuild": "^1.8.4",
    "electronmon": "^0.4.0",
    "eslint": "^5.16.0",
    "figures": "^3.0.0",
    "form-data": "^2.3.3",
    "mocha": "^6.1.4",
    "rootrequire": "^1.0.0",
    "spectron": "^5.0.0",
    "tar": "^4.4.8",
    "tempy": "^0.3.0",
    "unzipper": "^0.10.0",
    "xvfb-maybe": "^0.2.1",
    "yargs-parser": "^13.1.0"
  }
